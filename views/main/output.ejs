  <script src="js/webflow.js" type="text/javascript"></script>
  <script>
    // Progressive image loading
    document.addEventListener('DOMContentLoaded', function() {
        var lazyImages = document.querySelectorAll('.lazy-image');

        function markLoadedIfComplete(img){
          if (img.complete && img.naturalWidth > 0) {
            img.classList.add('loaded');
          }
        }

        // Always attach a load handler so cached/fallback loads also mark as loaded
        lazyImages.forEach(img => {
          img.addEventListener('load', function(){
            this.classList.add('loaded');
          });
          // If already loaded (cache), mark immediately
          markLoadedIfComplete(img);
        });
        
        if ('IntersectionObserver' in window) {
            const imageObserver = new IntersectionObserver((entries, observer) => {
                entries.forEach(entry => {
                    if (entry.isIntersecting) {
                        const img = entry.target;
                        // If it has already loaded via cache/fallback, mark it
                        markLoadedIfComplete(img);
                        observer.unobserve(img);
                    }
                });
            });

            lazyImages.forEach(img => imageObserver.observe(img));
        } else {
            // Fallback for older browsers
            lazyImages.forEach(markLoadedIfComplete);
        }
    });

    // Initialize intersection observer for dynamic content
    function initLazyLoading(container) {
        const images = container.querySelectorAll('.lazy-image:not(.loaded)');
        if ('IntersectionObserver' in window) {
            const imageObserver = new IntersectionObserver((entries, observer) => {
                entries.forEach(entry => {
                    if (entry.isIntersecting) {
                        const img = entry.target;
                        img.onload = () => img.classList.add('loaded');
                        observer.unobserve(img);
                    }
                });
            });

            images.forEach(img => imageObserver.observe(img));
        }
    }

    // button click 
    $('.is24.is-ab-card').on('click', function () {
      $('body').toggleClass('no-scroll');
      // Initialize lazy loading for newly visible content
      setTimeout(() => initLazyLoading(this), 100);
    });
    // button click 
    $('.link-block-3.is-bt-close').on('click', function () {
      $('body').toggleClass('no-scroll');
    }); 

    // Robust download fallback for all posts
    (function(){
      function head(url){
        return fetch(url, { method: 'HEAD' });
      }
      function tryAlternatives(anchor){
        const currentSlug = anchor.getAttribute('data-current-slug') || '';
        const legacySlug = anchor.getAttribute('data-legacy-slug') || '';
        const filename = anchor.getAttribute('data-filename') || '';
        const base = '/uploads/Update/';
        const primary = `${base}${currentSlug}/${filename}`;
        const alternatives = [
          `${base}${legacySlug}/${filename}`
        ];
        return head(primary).then(res=>{
          if(res.ok){ return primary; }
          throw new Error('primary missing');
        }).catch(()=>{
          let idx = 0;
          return new Promise((resolve, reject)=>{
            const next = ()=>{
              if(idx >= alternatives.length) return reject(new Error('no alternatives'));
              const url = alternatives[idx++];
              head(url).then(r=>{ if(r.ok) resolve(url); else next(); }).catch(next);
            };
            next();
          });
        });
      }
      document.addEventListener('click', function(e){
        const a = e.target.closest('a.download-link');
        if(!a) return;
        e.preventDefault();
        tryAlternatives(a).then(url=>{
          a.href = url;
          window.location.href = url;
        }).catch(()=>{
          window.location.href = a.href;
        });
      });
    })();
  </script>
