                                    const type = person.type ? person.type.toLowerCase() : '';
                                    
                                    // If type is a basin, return only that basin
                                    if (isBasinType(type)) {
                                      return [type];
                                    }
                                    
                                    // If type is 'core' or other, check description for basins
                                    return detectBasinsFromDescription(person.description);
                                  }
